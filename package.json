{
  "name": "showspark",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/codygordon/showspark.git"
  },
  "homepage": "https://github.com/codygordon/showspark",
  "author": "Cody Gordon",
  "license": "UNLICENSED",
  "engines": {
    "node": "v7.7.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "body-parser": "latest",
    "dotenv": "^2.0.0",
    "express": "4.13.3",
    "express-jwt": "^5.1.0",
    "express-rate-limit": "^2.6.0",
    "jwks-rsa": "^1.1.1",
    "mongodb": "latest",
    "morgan": "^1.8.1",
    "node-geocoder": "^3.16.0",
    "request": "^2.79.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js --exec babel-node --presets es2015,stage-0",
    "client": "babel-node start-client.js",
    "heroku-postbuild": "cd client/ && npm install --only=dev && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "latest",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  }
}
