{
  "name": "showspark",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/codygordon/showspark.git"
  },
  "homepage": "https://github.com/codygordon/showspark",
  "author": "Cody Gordon",
  "license": "UNLICENSED",
  "engines": {
    "node": "v7.7.0"
  },
  "dependencies": {
    "@turf/turf": "^4.4.0",
    "body-parser": "latest",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "express-jwt": "^5.1.0",
    "express-promise-router": "^1.1.1",
    "express-rate-limit": "^2.6.0",
    "heroku-ssl-redirect": "0.0.4",
    "jwks-rsa": "^1.1.1",
    "mongoose": "^4.10.6",
    "morgan": "^1.8.1",
    "node-geocoder": "^3.16.0",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.4",
    "sphere-knn": "^1.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "latest",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "jsonwebtoken": "^7.4.1",
    "mongodb": "^2.2.28",
    "node-sass-chokidar": "^0.0.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  },
  "scripts": {
    "test": "jest --coverage --watch",
    "build-css": "node-sass-chokidar client/src/styles -o client/src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar client/src -o client/src --watch --recursive",
    "start": "concurrently \"npm run server\" \"npm run client\" \"npm run watch-css\"",
    "server": "nodemon server.js --exec babel-node --presets es2015,stage-0 --optimize_for_size --max_old_space_size=460 --gc_interval=100",
    "client": "babel-node start-client.js",
    "heroku-postbuild": "npm install --only=dev && npm run build-css && cd client/ && npm install --only=dev && npm install && npm run build",
    "sync-dev-db": "babel-node data-processing/sync-to-local-db.js --presets es2015,stage-0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ]
}
