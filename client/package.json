{
  "name": "showspark-client",
  "author": "Cody Gordon",
  "license": "UNLICENSED",
  "version": "0.0.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "devDependencies": {
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "auth0-lock": "^10.7.2",
    "es6-promise": "^4.0.5",
    "history": "^4.2.0",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.11",
    "normalizr": "^2.3.0",
    "query-string": "^4.3.2",
    "re-base": "^2.2.0",
    "react": "latest",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-google-maps": "^6.0.1",
    "react-helmet": "^5.0.2",
    "react-icons": "^2.2.1",
    "react-mapbox-gl": "^1.4.0",
    "react-paginate": "^4.2.0",
    "react-places-autocomplete": "^4.1.1",
    "react-redux": "^4.4.6",
    "react-router": "latest",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-saga": "^0.13.0",
    "redux-thunk": "^2.1.0",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.61.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "import/extensions": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": false,
          "optionalDependencies": false,
          "peerDependencies": false
        }
      ],
      "import/prefer-default-export": "warn",
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "res|req|next|^err"
        }
      ],
      "no-shadow": [
        "error",
        {
          "allow": [
            "res",
            "req",
            "err"
          ]
        }
      ],
      "no-unused-expressions": [
        "error",
        {
          "allowTernary": true
        }
      ],
      "no-console": "warn",
      "no-underscore-dangle": "off",
      "comma-dangle": "off",
      "semi": "off",
      "radix": "off",
      "prefer-const": "warn",
      "consistent-return": "warn",
      "max-len": [
        1,
        180,
        4
      ],
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prefer-stateless-function": "warn",
      "react/prop-types": "warn"
    }
  }
}
